package edu.thu.cs;

public class Course {
	public int id = 0;
	public String name = null;
	public String teacher = null; 
	public String classtime = null;
	public int capacity = 0;
	public int load = 0;

	public String content = null;
	public boolean valid()
	{
		return (name!=null)&&(content!=null)&&(teacher!=null);
	}
}
